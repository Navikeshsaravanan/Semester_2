import java.util.Scanner;
public class Main {
        public static void main(String[] args) {
            Scanner scanner = new Scanner(System.in);
            System.out.print("Enter no: ");
            int num = scanner.nextInt();
            for (int i =1;i<=num;i++){
                for(int j=1;j<i+1;j++){
                    System.out.print("*");
                }
                System.out.println();
            }
            scanner.close();

        }
}
